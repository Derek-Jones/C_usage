
#ifndef __STD_IOMANIP
#define __STD_IOMANIP

#include <ios>

namespace std 
{

   unsigned int resetiosflags(ios_base::fmtflags mask);
   unsigned int setiosflags(ios_base::fmtflags mask);
   unsigned int setbase(int base);
